{
  "name": "@wkovacs64/eslint-config-ts-react",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 ESLint config (TypeScript and React)",
  "keywords": [
    "eslint",
    "eslintconfig",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/eslint-config-ts-react.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-config-ts-react/issues"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "validate": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wkovacs64/eslint-config-react": "1.0.3",
    "@wkovacs64/eslint-config-ts": "1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@typescript-eslint/parser": "1.3.0",
    "babel-eslint": "10.0.1",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.2.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "semantic-release": "15.13.3",
    "typescript": "3.3.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "typescript": "^3.3.1"
  }
}
