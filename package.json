{
  "name": "@wkovacs64/eslint-config-ts-react",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 ESLint config (TypeScript and React)",
  "keywords": [
    "eslint",
    "eslintconfig",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/eslint-config-ts-react.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-config-ts-react/issues"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "validate": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wkovacs64/eslint-config-react": "^3.0.0",
    "@wkovacs64/eslint-config-ts": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.13.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "semantic-release": "15.13.18",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "typescript": "^3.3.3"
  }
}
